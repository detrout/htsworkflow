    "biosample_term_id": {
      "@type": "@id",
      "description": "Ontology identifier describing biosample.",
      "pattern": "^(UBERON|EFO|CL|NTR|FBbt|WBbt):[0-9]{2,8}$",
      "type": "string",
      "title": "Ontology ID",
      "comment": "NTR is a new term request identifier provided by the DCC."
