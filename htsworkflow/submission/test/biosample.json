      "items": {
        "type": "string",
        "enum": [
          "head",
          "limb",
          "salivary gland",
          "male accessory sex gland",
          "testis",
          "female gonad",
          "digestive system",
          "arthropod fat body",
          "antenna",
          "adult maxillary segment",
          "female reproductive system",
          "male reproductive system"
        ]
      }
    },
    "model_organism_mating_status": {
      "description": "The mating status of the animal.",
      "type": "string",
      "title": "Mating status",
      "enum": [
        "mated",
        "virgin",
        "sterile",
        "mixed"
      ]
    },
