    "biosample_term_name": {
      "description": "Ontology term describing biosample.",
      "type": "string",
      "title": "Ontology term"
    },
    "biosample_type": {
      "description": "The categorization of the biosample.",
      "type": "string",
      "title": "Biosample type",
      "enum": [
        "primary cell",
        "immortalized cell line",
        "tissue",
        "in vitro differentiated cells",
        "induced pluripotent stem cell line",
        "whole organisms",
        "stem cell"
      ]
