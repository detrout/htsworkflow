  "properties": {
    "documents": {
      "uniqueItems": true,
      "description": "Documents that describe the biosample preparation.",
      "default": [],
      "type": "array",
      "items": {
        "description": "A document that provides additional information (not data file).",
        "type": "string",
        "linkTo": "Document",
        "title": "Document",
        "comment": "See document.json for available identifiers."
      },
      "title": "Documents"
