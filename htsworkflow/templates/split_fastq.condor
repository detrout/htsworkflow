Universe=vanilla
executable={{ python }}
error={{ logdir }}/fastq.$(process).out
output={{ logdir }}/fastq.$(process).out
log={{ logdir }}/fastq.log
{% if env %}environment="{{env}}"{% endif %}

{% for arg in args %}
arguments="{{ arg.pyscript }} -o {{ arg.target }} {% for s in arg.sources %}{{ s }} {% endfor %}"
queue
{% endfor %}
